{
  "name": "constructicon",
  "version": "4.4.20-0",
  "description": "Library of re-usable components for Professional Services projects",
  "main": "index.js",
  "scripts": {
    "lint": "prettier 'source/**/*.js' --check",
    "test": "mocha source/**/**/__tests__/*.js --recursive --ui bdd --require @babel/register --require test/common.js",
    "format": "prettier 'source/**/*.js' --write",
    "precommit": "lint-staged",
    "build": "babel source -d dist --copy-files",
    "build:icons": "svgo -f ./icons; tail -n +1 ./icons/*.svg > ./icons.xml",
    "prepare-package": "cp -R dist/components ./npm-constructicon && cp -R dist/lib ./npm-constructicon && sed -i '' 's/..\\/..\\/lib/..\\/lib/g' ./npm-constructicon/**/*.js && cp package.json readme.md ./npm-constructicon",
    "deploy:prepare": "npm run build && npm run prepare-package",
    "announce": "./bin/announce",
    "playground:start": "yarn webpack serve",
    "playground:build": "yarn webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackbaud-services/constructicon.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud-services/constructicon/issues"
  },
  "author": "Dan Henderson <dan.henderson@blackbaud.com>",
  "license": "MIT",
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/server": "11.11.0",
    "axios": "^0.21.4",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "query-string": "7.0.1",
    "react-avatar-editor": "13.0.2",
    "react-contenteditable": "^3.3.6",
    "react-dropzone": "^11.3.5",
    "react-modal": "^3.14.3",
    "react-onclickoutside": "^6.11.2",
    "react-slick": "^0.28.1",
    "react-slider": "^1.3.1",
    "react-ticker": "^1.2.2",
    "webpack-cli": "^5.1.4",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "7.23.7",
    "babel-loader": "9.1.3",
    "chai": "^4.3.4",
    "css-loader": "6.10.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "8.56.0",
    "husky": "^7.0.2",
    "jsdom": "^17.0.0",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "mocha": "^9.1.1",
    "moxios": "^0.4.0",
    "prettier": "^2.4.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.0",
    "sinon": "^11.1.2",
    "source-map-loader": "5.0.0",
    "style-loader": "3.3.4",
    "svgo": "3.2.0",
    "webpack": "^5.88.2",
    "webpack-dev-server": "^5.0.2"
  },
  "resolutions": {
    "@emotion/server/html-tokenize": "2.0.1",
    "eslint/**/minimatch": "3.0.5",
    "react-avatar-editor/**/@babel/traverse": "7.23.2",
    "webpack-dev-server/**/ansi-regex": "5.0.1",
    "webpack-dev-server/**/follow-redirects": "1.14.7"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0",
    "react-helmet": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "source/**/*.js": "prettier --check"
  }
}
